{
  "name": "items-management",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/api/index.js",
    "production": "node dist/api/index.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"client,server\" -c \"yellow,cyan\" \"npm run serve:client\" \"npm run serve:server\"",
    "build": "rimraf dist/ && npm run build:server && npm run build:client",
    "postinstall": "NODE_ENV=production npm run build",
    "pg-start": "pg_ctl -D /usr/local/var/postgres start",
    "pg-stop": "pg_ctl -D /usr/local/var/postgres stop",
    "migrate": "knex --knexfile ./knexfile.js --cwd . migrate:latest",
    "migrate:rollback": "knex --knexfile ./knexfile.js --cwd . migrate:rollback",
    "migrate:make": "knex --knexfile ./knexfile.js --cwd . migrate:make --",
    "seed": "knex seed:run",
    "lint:client": "eslint src/ui",
    "serve:client": "webpack-dev-server --config=webpack.config.js",
    "build:client": "webpack -p --config=webpack.config.js",
    "lint:server": "eslint src/api migrations seeds",
    "serve:server": "nodemon src/api/index.js --watch src/api --exec babel-node",
    "build:server": "babel src/api -d dist/api",
    "lint": "npm run lint:client && npm run lint:server"
  },
  "engines": {
    "node": "8.7.0",
    "npm": "5.5.1"
  },
  "private": true,
  "devDependencies": {
  },
  "dependencies": {
    "babel-eslint": "~7.1.1",
    "babel-loader": "~6.4.1",
    "concurrently": "~3.5.0",
    "eslint": "~3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-react": "~6.10.0",
    "file-loader": "~0.10.1",
    "nodemon": "~1.11.0",
    "style-loader": "~0.14.1",
    "stylus": "~0.54.5",
    "webpack": "~2.6.1",
    "webpack-dev-server": "~2.4.5",
    "babel-cli": "~6.24.0",
    "babel-core": "~6.24.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "~6.23.0",
    "babel-preset-es2015": "~6.24.0",
    "babel-preset-react": "~6.23.0",
    "babel-preset-stage-0": "~6.22.0",
    "babel-register": "~6.24.0",
    "body-parser": "~1.17.1",
    "bufferutil": "~3.0.0",
    "classnames": "~2.2.5",
    "connect-session-knex": "~1.3.4",
    "cors": "~2.8.1",
    "css-loader": "~0.28.7",
    "dotenv": "~4.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "express": "~4.15.2",
    "express-session": "~1.15.1",
    "html-webpack-plugin": "^2.30.1",
    "immutable": "~3.8.1",
    "isomorphic-fetch": "~2.2.1",
    "joi": "~10.2.2",
    "knex": "~0.12.8",
    "lodash": "~4.17.4",
    "nib": "~1.1.2",
    "passport": "~0.3.2",
    "passport-bnet": "~1.2.1",
    "pg": "~6.1.4",
    "pre-commit": "~1.2.2",
    "react": "~15.4.2",
    "react-dom": "~15.4.2",
    "react-immutable-proptypes": "~2.1.0",
    "react-ladda": "~5.0.6",
    "react-redux": "~5.0.3",
    "react-router": "~3.0.2",
    "react-router-redux": "~4.0.8",
    "react-textarea-autosize": "~4.0.5",
    "redux": "~3.6.0",
    "redux-logger": "~2.8.2",
    "redux-thunk": "~2.2.0",
    "rimraf": "^2.6.2",
    "stylus-loader": "~3.0.0",
    "utf-8-validate": "~3.0.1",
    "ws": "~2.2.1",
    "xml2json": "^0.11.0"
  },
  "pre-commit": {
    "run": [
      "lint"
    ]
  }
}
