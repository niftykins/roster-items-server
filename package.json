{
  "name": "items-server",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.js --watch src --exec babel-node",
    "full-lint": "eslint src migrations seeds",
    "lint": "yarn run full-lint -- --cache",
    "seed": "knex seed:run",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make --",
    "pg-start": "pg_ctl -D /usr/local/var/postgres start",
    "pg-stop": "pg_ctl -D /usr/local/var/postgres stop",
    "production": "babel-node src/index.js"
  },
  "engines": {
    "node": "6.10.0",
    "yarn": "0.21.3",
    "npm": "3.10.10"
  },
  "private": true,
  "devDependencies": {
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.10.0",
    "nodemon": "1.11.0"
  },
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "6.23.0",
    "body-parser": "1.16.1",
    "bufferutil": "2.0.1",
    "connect-session-knex": "1.3.4",
    "cors": "2.8.1",
    "dotenv": "4.0.0",
    "express": "4.14.1",
    "express-session": "1.15.1",
    "isomorphic-fetch": "2.2.1",
    "joi": "10.2.2",
    "knex": "0.12.7",
    "lodash": "4.17.4",
    "passport": "0.3.2",
    "passport-bnet": "1.2.1",
    "pg": "6.1.2",
    "pre-commit": "1.2.2",
    "utf-8-validate": "3.0.1",
    "ws": "2.2.0",
    "xml2json": "0.11.0"
  },
  "pre-commit": {
    "run": [
      "full-lint"
    ]
  }
}
